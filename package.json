{
  "name": "super-store-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=admin-dashboard\" \"npm run dev --workspace=storefront\" \"npm run dev --workspace=backend-api\"",
    "build": "npm run build --workspaces --if-present",
    "start": "npm run start --workspaces --if-present"
  },
  "dependencies": {
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "overrides": {
    "react": "19.2.3",
    "react-dom": "19.2.3"
  }
}